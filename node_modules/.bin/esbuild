#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bauricenafack/Documents/project/svelte/svelte_tailwind_tuto/node_modules/esbuild/bin/node_modules:/home/bauricenafack/Documents/project/svelte/svelte_tailwind_tuto/node_modules/esbuild/node_modules:/home/bauricenafack/Documents/project/svelte/svelte_tailwind_tuto/node_modules:/home/bauricenafack/Documents/project/svelte/node_modules:/home/bauricenafack/Documents/project/node_modules:/home/bauricenafack/Documents/node_modules:/home/bauricenafack/node_modules:/home/node_modules:/node_modules:/home/bauricenafack/Documents/project/svelte/svelte_tailwind_tuto/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/bauricenafack/Documents/project/svelte/svelte_tailwind_tuto/node_modules/esbuild/bin/node_modules:/home/bauricenafack/Documents/project/svelte/svelte_tailwind_tuto/node_modules/esbuild/node_modules:/home/bauricenafack/Documents/project/svelte/svelte_tailwind_tuto/node_modules:/home/bauricenafack/Documents/project/svelte/node_modules:/home/bauricenafack/Documents/project/node_modules:/home/bauricenafack/Documents/node_modules:/home/bauricenafack/node_modules:/home/node_modules:/node_modules:/home/bauricenafack/Documents/project/svelte/svelte_tailwind_tuto/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
